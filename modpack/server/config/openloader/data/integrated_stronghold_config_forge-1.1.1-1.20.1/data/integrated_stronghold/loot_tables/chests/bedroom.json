        {
          "type": "minecraft:item",
          "weight": 1,
          "name": "minecraft:written_book",
          "functions": [
            {
              "function":"set_nbt",
        "tag":"{
          pages:[
              \"[
                \\\"\\\",
                {\\\"text\\\":\\\"§kREDACTED\\\"},
                {\\\"text\\\":\\\"§l的日记\n第164天\n\n\\\"},
                {\\\"text\\\":\\\"我刚从 \\\"},
                {\\\"text\\\":\\\"§kREDACTED\\\"},
                {\\\"text\\\":\\\"来到要塞，就被他们热情的欢迎了。\\\"},
                {\\\"text\\\":\\\"这个强大的要塞提供了针对 \\\"},
                {\\\"text\\\":\\\"§kREDACTED\\\"},
                {\\\"text\\\":\\\"的危险的相当的保护。\\\"},
                {\\\"text\\\":\\\".\\\"}
              ]\",
              \"[
                {\\\"text\\\":\\\"然而，他们不停在说着至高无上者。\\\"},
                {\\\"text\\\":\\\"他们非常坚持让我相信他们的事业。\\\"}
              ]\"
            ],
            title:Journal,
            author:Unknown,
            display:{Lore:[\"Found in the ancient Stronghold\"]}
            }"},
            {
              "function": "minecraft:set_name",
              "name": "神秘日记 #1",
              "add": false
            }
          ]
        },
        {
          "type": "minecraft:item",
          "weight": 1,
          "name": "minecraft:written_book",
          "functions": [
            {
              "function":"set_nbt",
        "tag":"{
          pages:[
              \"[
                \\\"\\\",
                {\\\"text\\\":\\\"§l冒险家\\\"},
                {\\\"text\\\":\\\"§l的日记\n第162天\n\n\\\"},
                {\\\"text\\\":\\\"我来参观这个强大的要塞了。 \\\"},
                {\\\"text\\\":\\\"这是一个科技和建筑学的奇观。\\\"}
              ]\",
              \"[
                {\\\"text\\\":\\\"我是经过一番盘问才进来的。他们有很多安全措施。 \\\"},
                {\\\"text\\\":\\\"我想知道他们在担心地表上的什么。\\\"}
              ]\"
            ],
            title:Journal,
            author:Unknown,
            display:{Lore:[\"Found in the ancient Stronghold\"]}
            }"},
            {
              "function": "minecraft:set_name",
              "name": "神秘日记 #2",
              "add": false
            }
          ]
        },
        {
          "type": "minecraft:item",
          "weight": 1,
          "name": "minecraft:written_book",
          "functions": [
            {
              "function":"set_nbt",
        "tag":"{
          pages:[
              \"[
                \\\"\\\",
                {\\\"text\\\":\\\"§l圣经\n\n\\\"},
                {\\\"text\\\":\\\"我们有一个目标：\n\\\"},
                {\\\"text\\\":\\\"通过建筑\n\\\"},
                {\\\"text\\\":\\\"来为世界\n\\\"},
                {\\\"text\\\":\\\"注入美丽和活力。\n\\\"},
                {\\\"text\\\":\\\"我们目前的\n\\\"},
                {\\\"text\\\":\\\"形态\n\\\"},
                {\\\"text\\\":\\\"限制了我们实现想象的能力。\n\\\"},
                {\\\"text\\\":\\\"为了最大程度的开发我们的潜能，\n\\\"},
                {\\\"text\\\":\\\"我们必须\n\\\"},
                {\\\"text\\\":\\\"达到\\\"},
                {\\\"text\\\":\\\"§o末地 \\\"},
                {\\\"text\\\":\\\"形态。\\\"}
              ]\",
              \"[
                {\\\"text\\\":\\\"§o末地 \\\"},
                {\\\"text\\\":\\\"形态是我们进化的下个阶段。\n\n\\\"},
                {\\\"text\\\":\\\"我们的潜能将被完全激发。 \\\"},
                {\\\"text\\\":\\\"我们的凡躯将不能束缚我们，我们将可以随心所欲的建造。\\\"},
                {\\\"text\\\":\\\"§o末地 \\\"},
                {\\\"text\\\":\\\"将它带给我们的是\\\"},
                {\\\"text\\\":\\\"§o至高无上者。\\\"},
                {\\\"text\\\":\\\"她需要我们，需要她的追随者，来实现她的目标。\\\"}
              ]\",
              \"[
                {\\\"text\\\":\\\"有了我们的帮助，\n\n\\\"},
                {\\\"text\\\":\\\"§o至高无上者\\\"},
                {\\\"text\\\":\\\"将会熄灭森林的大火...\n\n\\\"},
                {\\\"text\\\":\\\"她将终结所有的饥荒...\n\n\\\"},
                {\\\"text\\\":\\\"她将拯救那些生病和贫穷的人...\\\"}
              ]\",
              \"[
                {\\\"text\\\":\\\"她会阻止\n\\\"},
                {\\\"text\\\":\\\"§kundead\\\"},
                {\\\"text\\\":\\\"的崛起...\n\n\\\"},
                {\\\"text\\\":\\\"她会将我们带到新的高度。\n\n\\\"},
                {\\\"text\\\":\\\"我们会一同将这个世界变成所有人的避难所，\\\"},
                {\\\"text\\\":\\\"远离暴力，远离苦难，远离\\\"},
                {\\\"text\\\":\\\"§kREDACTED\\\"}
              ]\",
              \"[
                {\\\"text\\\":\\\"§n我们知晓这些。\n\n\\\"},
                {\\\"text\\\":\\\"§o我们已经从天上的石头里预见了这一切。\\\"}
              ]\",
              \"[
                {\\\"text\\\":\\\"§o她在等待着我们。\n\n\\\"},
                {\\\"text\\\":\\\"§o她用雷鸣般的吼声等待着我们。\n\n\\\"},
                {\\\"text\\\":\\\"§o她用神奇的呼吸等待着我们。\n\n\\\"},
                {\\\"text\\\":\\\"§o她在高空飞翔，等待着我们。\\\"}
              ]\",
              \"[
                {\\\"text\\\":\\\"我们会找到她的。\\\"}
              ]\"
            ],
            title:Scripture,
            author:Priest,
            display:{Lore:[\"Found in the ancient Stronghold\"]}
            }"},
            {
              "function": "minecraft:set_name",
              "name": "龙之圣经",
              "add": false
            }
          ]
        }
      ]
