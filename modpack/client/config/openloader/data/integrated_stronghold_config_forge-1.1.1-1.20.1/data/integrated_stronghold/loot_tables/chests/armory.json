              "function":"set_nbt",
        "tag":"{
          pages:[
              \"[
                \\\"\\\",
                {\\\"text\\\":\\\"§l金属辊压机说明书\n\\\"},
                {\\\"text\\\":\\\"§l作者：\\\"},
                {\\\"text\\\":\\\"§kREDACTED\n\n\\\"},
                {\\\"text\\\":\\\"操作辊压机相当简单。 \\\"},
                {\\\"text\\\":\\\"将金属锭放在置物台上，然后转动曲柄来辊压金属锭。\\\"}
              ]\",
              \"[
                {\\\"text\\\":\\\"请注意不要把任何不需要的物品放上去，也包括你自己！会受伤的！\\\"}
              ]\"
            ],
