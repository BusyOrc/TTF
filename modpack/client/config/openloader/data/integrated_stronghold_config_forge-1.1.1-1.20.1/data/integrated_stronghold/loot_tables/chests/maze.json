        {
          "type": "minecraft:item",
          "weight": 1,
          "name": "minecraft:written_book",
          "functions": [
            {
              "function":"set_nbt",
        "tag":"{
          pages:[
              \"[
                \\\"\\\",
                {\\\"text\\\":\\\"§kREDACTED\\\"},
                {\\\"text\\\":\\\"§l的日记\n\n\\\"},
                {\\\"text\\\":\\\"我沿着魔法之眼的踪迹，来到了这个黑暗而奇妙的要塞。\\\"},
                {\\\"text\\\":\\\"它早已被遗弃，并被亡灵占领。\\\"},
                {\\\"text\\\":\\\"我发现了一些奇怪的古代日志，表明这里曾是一种古代建造者的家园。\\\"}
              ]\",
              \"[
                {\\\"text\\\":\\\"他们有一些信息被\\\"},
                {\\\"text\\\":\\\"模糊的语言所掩盖。\\\"},
                {\\\"text\\\":\\\"不知道是什么黑魔法造成的。\\\"}
              ]\"
            ],
            title:Journal,
            author:Unknown,
            display:{Lore:[\"Found in the ancient Stronghold\"]}
            }"},
            {
              "function": "minecraft:set_name",
              "name": "旅行者的日记 #1",
              "add": false
            }
          ]
        },
        {
          "type": "minecraft:item",
          "weight": 1,
          "name": "minecraft:written_book",
          "functions": [
            {
              "function":"set_nbt",
        "tag":"{
          pages:[
              \"[
                \\\"\\\",
                {\\\"text\\\":\\\"§kREDACTED\\\"},
                {\\\"text\\\":\\\"§l的日记\n\n\\\"},
                {\\\"text\\\":\\\"这个迷宫是这个黑暗要塞中最险恶的地方。\\\"},
                {\\\"text\\\":\\\"我差点被陷阱里的炸弹炸死。我开始失去活着出去的希望了。\\\"},
                {\\\"text\\\":\\\"我要尝试折返，找到来时的路。\\\"}
              ]\",
              \"[
                {\\\"text\\\":\\\"如果你正在读这篇文章，那说明我没有成功。\\\"}
              ]\"
            ],
            title:Journal,
            author:Unknown,
            display:{Lore:[\"Found in the ancient Stronghold\"]}
            }"},
            {
              "function": "minecraft:set_name",
              "name": "旅行者的日记 #2",
              "add": false
            }
          ]
        },
        {
          "type": "minecraft:item",
          "weight": 1,
          "name": "minecraft:written_book",
          "functions": [
            {
              "function":"set_nbt",
        "tag":"{
          pages:[
              \"[
                \\\"\\\",
                {\\\"text\\\":\\\"§kREDACTED\\\"},
                {\\\"text\\\":\\\"§l的日记\n\n\\\"},
                {\\\"text\\\":\\\"曲折和无尽的黑暗空间快把我逼疯了。\\\"},
                {\\\"text\\\":\\\"我只能与黑暗中的怪物战斗这么长时间。 \\\"}
              ]\",
              \"[
                {\\\"text\\\":\\\"这个迷宫可能是我寻找魔法传送门的终点，也是我生命的终点。\\\"}
              ]\"
            ],
            title:Journal,
            author:Unknown,
            display:{Lore:[\"Found in the ancient Stronghold\"]}
            }"},
            {
              "function": "minecraft:set_name",
              "name": "旅行者的日记 #3",
              "add": false
            }
          ]
        }
